{
  "compilerOptions":{
    "target":"ES6",
    "module":"ESNext",
    "jsx":"react-jsx",
    "moduleResolution":"node",
    "esModuleInterop": true,
    "strict":true,
    "skipLibCheck":true,
    "forceConsistentCasinglnFileNames":true,
    "resolveJsonModule":true,
    "isolatedModules":true,
    "noEmit":true,
    "types": ["react","react-dom"]
  },
  "include": ["src"]
  }
  